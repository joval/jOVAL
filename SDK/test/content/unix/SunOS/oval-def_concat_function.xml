      <value>abc123</value>
      <value>abc456</value>
      <value>def123</value>
      <value>def456</value>
    </constant_variable>
    <constant_variable id="oval:org.mitre.oval.test:var:393" version="1" comment="The results of the concat function on three multi-value-variables." datatype="string">
      <value>abc123uvw</value>
      <value>abc123xyz</value>
