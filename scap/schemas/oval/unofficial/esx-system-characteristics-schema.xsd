                                <xsd:documentation>The name of a vSphere Distributed Switch.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="portgroup_name" type="oval-sc:EntityItemStringType" minOccurs="0" maxOccurs="1">
