    <!-- =============================================================================== -->
    <!-- Virtual Distributed Port Group Item -->
    <!-- =============================================================================== -->
    <xsd:element name="vds_portgroup_item" substitutionGroup="oval-sc:item">
        <xsd:annotation>
            <xsd:documentation></xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="oval-sc:ItemType">
                    <xsd:sequence>
                        <xsd:element name="vds_name" type="oval-sc:EntityItemStringType" minOccurs="0" maxOccurs="1">
